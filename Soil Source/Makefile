SRCS_FORd1 = \
2DSOILVine.FOR 

OBJS_FORd1 = \
2DSOILVine.o 

SRC_DIR_FORd1 = 
OBJS_DIR = obj/Debug/
EXE_DIR = bin/Debug/

EXE = 2DSOIL_Grape.exe
FC = gfortran.exe
LD = gfortran.exe
IDIR = 
CFLAGS = -Wall -g -fall-intrinsics  -J$(OBJS_DIR) $(IDIR)
LFLAGS = 
LIBS = 

VPATH = $(SRC_DIR_FORd1):$(OBJS_DIR)
OBJS = $(addprefix $(OBJS_DIR), $(OBJS_FORd1))

all : $(EXE)

$(EXE) : $(OBJS_FORd1)
	@mkdir -p $(EXE_DIR)
	$(LD) -o $(EXE_DIR)$(EXE) $(OBJS) $(LFLAGS) $(LIBS)

$(OBJS_FORd1):
	@mkdir -p $(OBJS_DIR)
	$(FC) $(CFLAGS) -c $(SRC_DIR_FORd1)$(@:.o=.FOR) -o $(OBJS_DIR)$@

clean :
	rm -f $(OBJS_DIR)*.*
	rm -f $(EXE_DIR)$(EXE)

# Dependencies of files
2DSOILVine.o: \
    2DSOILVine.FOR

